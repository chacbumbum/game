{
	"extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
	"parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
		"ecmaFeatures": {
            "jsx": true
        }
    },
	"plugins": [
        "react"
    ],
	"rules": {
        "no-undef": 0,
        "no-unused-vars": 0,
        "no-console": 0
    }
}